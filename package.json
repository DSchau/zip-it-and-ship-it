{
  "name": "@netlify/zip-it-and-ship-it",
  "description": "Zip it and ship it",
  "version": "0.3.1",
  "author": "Netlify, Inc",
  "ava": {
    "files": [
      "src/**/*.test.js",
      "pkg/**/*.test.js"
    ],
    "compileEnhancements": false,
    "babel": false
  },
  "bin": {
    "zip-it-and-ship-it": "./src/bin.js"
  },
  "bugs": {
    "url": "https://github.com/netlify/zip-it-and-ship-it/issues"
  },
  "dependencies": {
    "archiver": "^3.0.0",
    "debug": "^4.1.1",
    "elf-tools": "^1.1.1",
    "glob": "^7.1.3",
    "npm-packlist": "^1.1.12",
    "p-all": "^2.0.0",
    "precinct": "^6.1.1",
    "read-pkg-up": "^4.0.0",
    "require-package-name": "^2.0.1",
    "resolve": "^1.10.0",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/runtime": "^7.3.1",
    "adm-zip": "^0.4.13",
    "auto-changelog": "^1.12.0",
    "ava": "^2.4.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "common-path-prefix": "^2.0.0",
    "coveralls": "^3.0.6",
    "cpy": "^7.3.0",
    "del": "^5.1.0",
    "dependency-check": "^3.3.0",
    "end-of-stream": "^1.4.1",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "execa": "^2.0.4",
    "fast-glob": "^3.0.4",
    "gh-release": "^3.5.0",
    "is-ci": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.0.0",
    "path-exists": "^4.0.0",
    "pkg": "^4.4.0",
    "prettier": "^1.16.4",
    "readdirp": "^3.1.2",
    "tar-fs": "^2.0.0",
    "tmp-promise": "^2.0.2",
    "util.promisify": "^1.0.0"
  },
  "engines": {
    "node": ">=8.12.0"
  },
  "homepage": "https://github.com/netlify/zip-it-and-ship-it#README",
  "keywords": [
    "api",
    "bundler",
    "cli",
    "lambda",
    "lambda functions",
    "netlify",
    "static"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": "netlify/zip-it-and-ship-it",
  "scripts": {
    "prepublishOnly": "run-s build && git push && git push --tags && run-s assets",
    "build": "node pkg/main.js",
    "assets": "node pkg/github.js",
    "test": "run-s test:*",
    "test:lint": "eslint --fix \"{src/**/,pkg/,}*.js\"",
    "test:prettier": "prettier --write --loglevel warn \"{src,pkg}/**/*.js\" \"*.{js,md,yml,json}\"",
    "test:deps": "dependency-check ./package.json --unused --missing --no-dev --no-peer",
    "test:ava": "nyc --reporter=lcov ava --verbose && nyc report",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "version": "auto-changelog -p --template keepachangelog --breaking-pattern breaking && git add CHANGELOG.md"
  }
}
